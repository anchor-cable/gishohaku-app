{
  "hosting": {
    "public": "app/out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [ {
      "source": "/blog/:post_id",
      "destination": "https://mottox2.com/posts/:post_id",
      "type": 301
    }],
    "rewrites": [{
      "source": "/books/**",
      "function": "book"
    }, {
      "source": "/post2",
      "function": "post"
    }]
  },
  "functions": {
    "source": "functions"
  }
}